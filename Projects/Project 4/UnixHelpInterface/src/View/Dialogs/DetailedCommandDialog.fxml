<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<fx:root alignment="CENTER" maxHeight="Infinity" maxWidth="Infinity" spacing="15.0" type="VBox" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label fx:id="titleLabel" alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" />
      <GridPane hgap="5.0" vgap="15.0">
        <columnConstraints>
          <ColumnConstraints halignment="RIGHT" hgrow="SOMETIMES" maxWidth="75.0" prefWidth="75.0" />
          <ColumnConstraints hgrow="SOMETIMES" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="35.0" prefHeight="35.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="35.0" prefHeight="35.0" vgrow="SOMETIMES" />
          <RowConstraints maxHeight="1.7976931348623157E308" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="35.0" prefHeight="35.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="Name:*" />
            <Label text="Description:*" GridPane.rowIndex="1" />
            <Label alignment="TOP_RIGHT" contentDisplay="CENTER" text="Details:" GridPane.rowIndex="2" />
            <Label text="Options:" GridPane.rowIndex="3" />
            <Label text="Format:" GridPane.rowIndex="4" />
            <Label text="Examples:" GridPane.rowIndex="5" />
            <Label text="Link:" GridPane.rowIndex="6" />
            <TextField fx:id="nameTextField" minHeight="0.0" minWidth="0.0" promptText="Name" GridPane.columnIndex="1" />
            <TextField minHeight="0.0" minWidth="0.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <TextField minHeight="0.0" minWidth="0.0" GridPane.columnIndex="1" GridPane.rowIndex="6" />
            <TextArea prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="2" />
         </children>
      </GridPane>
      <HBox alignment="CENTER_RIGHT" spacing="40.0">
         <children>
            <Button mnemonicParsing="false" text="Save" />
            <Button mnemonicParsing="false" text="Cancel" onAction="#onCancelButtonClick" />
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</fx:root>
